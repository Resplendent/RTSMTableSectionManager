source 'https://github.com/CocoaPods/Specs.git'
use_frameworks!


def import_pods
	pod 'RTSMTableSectionManager', :path => '../'
end


target 'RTSMTableSectionManager_Example', :exclusive => true do
	import_pods
end

target 'RTSMTableSectionManager_Tests', :exclusive => true do
	import_pods
	
	pod 'Specta'
	pod 'Expecta'
	pod 'FBSnapshotTestCase'
	pod 'Expecta+Snapshots'
end
